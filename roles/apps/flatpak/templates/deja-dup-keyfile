[org/gnome/deja-dup]
backend='drive'

include-list=['$HOME']
exclude-list=['$TRASH', '$DOWNLOAD', '$HOME/.var', '$HOME/.local/share/flatpak', '$HOME/repos', '$HOME/sshfs']

periodic=true
periodic-period=7
delete-after=182

